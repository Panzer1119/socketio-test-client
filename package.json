    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "finalhandler": "^1.2.0",
    "http": "^0.0.1-security",
    "nanoid": "^4.0.0",
    "serve-static": "^1.15.0",
